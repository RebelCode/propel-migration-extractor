{
    "name": "rebelcode/propel-sql-extractor",
    "description": "A PHP library for extracting SQL from Propel migrations.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 | ^7.0",
        "dhii/exception-helper-base": "dev-develop",
        "dhii/i18n-helper-base": "dev-develop",
        "dhii/normalization-helper-base": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "dhii/stringable-interface": "^0.1",
        "adlawson/vfs": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\Migrations\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    }
}
